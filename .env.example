# Rocket Telemetry System Environment Configuration

# Database
DATABASE_URL=postgresql://telemetry_user:secure_password@localhost:5432/telemetry
DB_PASSWORD=secure_password

# MQTT Configuration
MQTT_BROKER_HOST=localhost
MQTT_BROKER_PORT=1883
MQTT_USERNAME=ground_station
MQTT_PASSWORD=mqtt_secure_pass
MQTT_TLS_ENABLED=true

# LoRa Configuration
LORA_FREQUENCY=915.0
LORA_SPREADING_FACTOR=7
LORA_BANDWIDTH=125
LORA_CODING_RATE=5

# Security
ENCRYPTION_KEY=your-16-byte-key-here
JWT_SECRET_KEY=your-jwt-secret-key
API_KEY=your-api-key-here

# Monitoring
PROMETHEUS_PORT=9091
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin

# Features
CAMERA_ENABLED=true
FAULT_INJECTION_ENABLED=true
FAULT_GPIO_PINS=17,27,22,23

# Logging
LOG_LEVEL=INFO